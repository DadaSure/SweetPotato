/* pages/welcome/welcome.wxss */
.pageContainer{
  /* width: auto; */
}
.titleContainer{
  margin-top: 29px;
  margin-bottom: 20px;
  margin-left: 24px;
  margin-right: 16px;
  width: auto;
  display: flex;
  justify-content: space-between;
  height: 40px;
  vertical-align: center;
}
.pageTitle{
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  font-size: 34px;
  font-weight: bold;
  color: #FF9746;
  width: 30%;
  position: relative;
  top: -3px;
}
.contentContainer{
  margin-left: 24px;
  margin-right: 16px;
  display: flex;
  align-content: flex-start;
  width: auto;
  flex-wrap: wrap;
}
.text{
  width: 100%;
  margin-top: 10px;
  font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  font-size: 17px;
  text-align: left;
  letter-spacing: 2px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.Button{
  margin-top: 200px;
  width: 240px;
  color: #FFFFFF;
  background-color:rgba(255, 151, 70, 0.8);
  border-color: rgba(0, 0, 0, 0);
  border-radius: 10px;
}